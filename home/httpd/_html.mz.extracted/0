<SCRIPT>
function pageGetValues()
{
var UpstreamInp = parseFloat(<%=UpstreamInp;%>)/10;
var DownstreamInp = parseFloat(<%=DownstreamInp;%>)/10;
var upatten = getValue("Upstream_attenuation");
upatten = upatten/10;
var downatten = getValue("Downstream_attenuation");
downatten = downatten/10;
var upnoise = getValue("Upstream_noise_margin");
upnoise = upnoise/10;
var downnoise = getValue("Downstream_noise_margin");
downnoise = downnoise/10;
var uppower = getValue("Upstream_power");
uppower = uppower/10;
var downpower = getValue("Downstream_power");
downpower = downpower/10;
var showtime = getValue("Showtime_start");
var days = 0;
var hours = 0;
var mins  = 0;
var secs  = 0;
days = Math.round(showtime/86400);
if(showtime/86400 < days)
{
days = days - 1;
}
showtime = Math.round(showtime% 86400);
hours = Math.round(showtime/3600);
if(showtime/3600 < hours)
{
hours = hours - 1;
}
showtime = Math.round(showtime%3600);
mins  = Math.round(showtime/60);
if(showtime/60 < mins)
{
mins = mins - 1;
}
secs  = Math.round(showtime%60);
getObj("Inp").innerHTML = UpstreamInp+"/"+DownstreamInp+" symbols";
getObj("Fnt_Upstream_noise_margin").innerHTML = upnoise;
getObj("Fnt_Downstream_noise_margin").innerHTML = downnoise;
getObj("Fnt_Upstream_attenuation").innerHTML = upatten;
getObj("Fnt_Downstream_attenuation").innerHTML = downatten;
getObj("Fnt_Upstream_power").innerHTML = uppower;
getObj("Fnt_Downstream_power").innerHTML = downpower;
getObj("Frm_Showtime_start").innerHTML = days + " days: " + hours + " hrs: " + mins + " min: "+ secs + " sec";
var UpstreamInp1 = parseFloat(<%=UpstreamInp1;%>) / 10;
var DownstreamInp1 = parseFloat(<%=DownstreamInp1;%>) / 10;
var upatten1 = getValue("Upstream_attenuation1");
upatten1 = upatten1 / 10;
var downatten1 = getValue("Downstream_attenuation1");
downatten1 = downatten1 / 10;
var upnoise1 = getValue("Upstream_noise_margin1");
upnoise1 = upnoise1 / 10;
var downnoise1 = getValue("Downstream_noise_margin1");
downnoise1 = downnoise1 / 10;
var uppower1 = getValue("Upstream_power1");
uppower1 = uppower1 / 10;
var downpower1 = getValue("Downstream_power1");
downpower1 = downpower1 / 10;
var showtime1 = getValue("Showtime_start1");
var days1 = 0;
var hours1 = 0;
var mins1  = 0;
var secs1  = 0;
days1 = Math.round(showtime1 / 86400);
if(showtime1 / 86400 < days1)
{
days1 = days1 - 1;
}
showtime1 = Math.round(showtime1 % 86400);
hours1 = Math.round(showtime1 / 3600);
if(showtime1 / 3600 < hours1)
{
hours1 = hours1 - 1;
}
showtime1 = Math.round(showtime1 % 3600);
mins1  = Math.round(showtime1 / 60);
if(showtime1 / 60 < mins1)
{
mins1 = mins1 - 1;
}
secs1  = Math.round(showtime1%60);
getObj("Inp1").innerHTML = UpstreamInp1 + "/" + DownstreamInp1 + " symbols";
getObj("Fnt_Upstream_noise_margin1").innerHTML = upnoise1;
getObj("Fnt_Downstream_noise_margin1").innerHTML = downnoise1;
getObj("Fnt_Upstream_attenuation1").innerHTML = upatten1;
getObj("Fnt_Downstream_attenuation1").innerHTML = downatten1;
getObj("Fnt_Upstream_power1").innerHTML = uppower1;
getObj("Fnt_Downstream_power1").innerHTML = downpower1;
getObj("Frm_Showtime_start1").innerHTML = days1 + " days: " + hours1 + " hrs: "
+ mins1 + " min: "+ secs1 + " sec";
}
