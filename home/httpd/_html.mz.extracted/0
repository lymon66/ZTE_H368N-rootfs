<script language="javascript">

var instNum = getValue("WLAN_INSTNUM");
var instFlag = new Array(4);
for (var i=0; i<4; i++)
{
instFlag[i] = 0;
}
var index = 0;
for (var i=0; i<instNum; i++)
{
var viewName = getValue("WLAN_SSID"+i);
switch(viewName)
{
case "IGD.LD1.WLAN1":
instFlag[0] = 1;
break;
case "IGD.LD1.WLAN2":
instFlag[1] = 1;
break;
case "IGD.LD1.WLAN3":
instFlag[2] = 1;
break;
case "IGD.LD1.WLAN4":
instFlag[3] = 1;
break;
};
}
var ssidObj = getObj("Frm_SSID_SET");
for (var i=0; i<4; i++)
{
if (instFlag[i] == 0)
{
var ssidOptObj = getObj("Frm_SSID_SET"+i);
ssidObj.removeChild(ssidOptObj);
}
}
</script>
