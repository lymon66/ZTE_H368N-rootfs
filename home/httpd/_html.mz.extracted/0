{
CurUpLinkType="DSL is training";
}
else if (MultiWDMode=="Auto")
{
CurUpLinkType="Detecting";
}
else
{
}
}
else if (CurUpLinkType==5)
{
CurUpLinkType="USB 3G Dongle";
}
else
{
CurUpLinkType="none";
