pageLoad();
function pageLoad()
{
var ss = getObj("EnWAN");
ss.focus();
<%
if(EN_ADSL == "1" && EN_VDSL == "1" && EN_ETH == "1" && EN_STB == "0")
{
%>
OK_WAN();
<%
}
else
{
%>
NO_WAN();
<%
}
%>
}
function WAN_Enable()
{
setValue("IF_ACTION", "WAN_CON");
jslDisable("EnWAN");
getObj("fSubmit").submit();
}
function NO_WAN()
{
var ss = document.getElementById("EnWAN");
ss.value = "NO";
ss.style.color = "#E8117C";
jslEnable("EnWAN");
}
function OK_WAN()
{
var ss = document.getElementById("EnWAN");
ss.value = "OK";
ss.style.color = "#279B0B";
jslDisable("EnWAN");
}
function Transfer_meaning(id,value)
{
document.getElementById(id).value=value;
}
</script>

