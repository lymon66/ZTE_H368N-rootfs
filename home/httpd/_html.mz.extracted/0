<script language="javascript">function pageLoad(){getObj("fSubmit").action=getURL("commontest_t.gch");var zzz=getValue("IF_ERRORSTR");if(zzz!="SUCC"){var xxx=getValue("IF_ERRORPARAM");if(xxx!="SUCC"){alert(xxx+zzz);}else {alert(zzz);}}else {var ret=getValue("IF_RESULT");if(ret=="0"){}else if(ret=="-1"||ret=="-2"){alert("username or password is error!");}}pageGetValue();}function objname_change(value){setValue("IF_OBJINDEX",value);setValue("IF_INSTINDEX",0);setValue("IF_PARANUM",0);setValue("IF_ACTION","NULL");getObj("fSubmit").submit();}function identity_change(value){setValue("IF_INSTINDEX",value);setValue("IF_PARANUM",0);setValue("IF_ACTION","NULL");getObj("fSubmit").submit();}function pageGetValue(){jslSetValue("obj_name","IF_OBJINDEX");jslSetValue("identity","IF_INSTINDEX");var para_num=getValue("IF_PARANUM");for(var i=0;i<para_num;i++){jslSetValue("para"+i,"if_para"+i);jslSetValue("paravalue"+i,"if_paravalue"+i);}}function pageSetValue(){var para_num=getValue("IF_PARANUM");for(var i=0;i<para_num;i++){jslEnable("para"+i);jslSetValue("if_para"+i,"para"+i);jslSetValue("if_paravalue"+i,"paravalue"+i);}}function myDoSubmitGeneral(){if(getValue("IF_OBJINDEX")<=0){alert("Please choose obj!");}else if(getValue("IF_INSTINDEX")<0){alert("Inst Index invalid!");}else {pageSetValue();setValue("IF_ACTION","apply");getObj("fSubmit").submit();}}function myDoDelSubmitGeneral(){if(getValue("IF_OBJINDEX")<=0){alert("Please choose obj!");}else {pageSetValue();setValue("IF_ACTION","del");getObj("fSubmit").submit();}}function myDoGetByParamSubmitGeneral(){if(getValue("IF_OBJINDEX")<=0){alert("Please choose obj!");}else {pageSetValue();setValue("IF_ACTION","getbyparam");getObj("fSubmit").submit();}}function DevSaveSubmit(){if(getValue("IF_OBJINDEX")>0){alert("Please don't choose obj!");}else {pageSetValue();setValue("IF_ACTION","devsave");getObj("fSubmit").submit();}}function DevRestartSubmit(){if(getValue("IF_OBJINDEX")>0){alert("Please don't choose obj!");}else {pageSetValue();setV