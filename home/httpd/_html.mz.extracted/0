function setinner(type)
{
document.getElementById("myLayer").style.visibility = type;
}
var USERINFO_PARA = new Array(
"Type",
"Enable",
"Username",
"Password",
"Right",
"IsChanged"
);
function getmsg(id)
{
var str     = new Array();
str[0]   = new Array(102, "&?error_102;");
str[1]   = new Array(103, "&?error_103;");
str[2]   = new Array(114, "&?error_114;");
str[3]   = new Array(118, "&?error_118;");
str[4]   = new Array(119, "&?error_119;");
str[5]   = new Array(122, "&?error_122;");
str[6]   = new Array(123, "&?error_123;");
str[7]   = new Array(124, "&?error_124;");
str[8]   = new Array(101, "&?error_101;");
return getMsgFormArray(str, arguments);
}
function pageLoad(url)
{
Doadmin(0);
var errstr= getValue("IF_ERRORSTR");
getObj("fSubmit").action = url;
pageGetValue();
if (errstr != "SUCC")
{
var errpara = getValue("IF_ERRORPARAM");
if (errpara != "SUCC")
{
if (document.getElementById("Fnt"+errpara) == null)
{
msg = getmsg(122);
ShowErrorForCom(null, null, msg);
}
else
{
msg = getmsg(118, "Fnt"+errpara);
ShowErrorForCom("Frm"+errpara, null, msg);
}
}
else
{
msg = getmsg(122);
ShowErrorForCom(null, null, msg);
}
}
}
function pageGetValue()
{
jslSetValue("Frm_Username", "Username");
}
function pageSetValue()
{
HiddenParaInit(USERINFO_PARA);
jslSetValue("Username", "Frm_Username");
jslSetValue("Password", "Frm_Password");
setValue("IsChanged", "1");
}
function pageCheckValue()
{
var msg = "";
var Frm_Username = getValue("Frm_Username");
var Frm_Password = getValue("Frm_Password");
var Frm_CfmPassword = getValue("Frm_CfmPassword");
if(Frm_Username == "" || Frm_Username.length == 0)
{
msg = getmsg(101);
ShowErrorForCom("Frm_Username", "Fnt_Username", msg);
return false;
}
if(Frm_Password == "" || Frm_Password.length == 0)
{
msg = getmsg(101);
ShowErrorForCom("Frm_Password", "Fnt_Password", msg);
return false;
}
if(checkGenStringForASC(Frm_Username) != true)
{
msg = getmsg(114);
ShowErrorForCom("Frm_Username", "Fnt_Username", msg);
return false;
}
if(checkStrLengthRange(Frm_Username, 1, 256) == -3)
{
msg = getmsg(102, 1, 256);
ShowErrorForCom("Frm_Username", "Fnt_Username", msg);
return false;
}
if(checkGenStringForASC(Frm_Password) != true)
{
msg = getmsg(114);
ShowErrorForCom("Frm_Password", "Fnt_Password", msg);
return false;
}
if(checkStrLengthRange(Frm_Password, 1, 256) == -3)
{
msg = getmsg(102, 1, 256);
ShowErrorForCom("Frm_Password", "Fnt_Password", msg);
return false;
}
if(Frm_Password != Frm_CfmPassword)
{
msg = getmsg(124);
ShowErrorForCom("Frm_CfmPassword", null, msg);
return false;
}
return true;
}
function pageSubmit()
{
if(pageCheckValue() == true)
{
jslDisable("Btn_Submit");
pageSetValue();
setValue("IF_ACTION", "apply");
getObj("fSubmit").submit();
}
}
function Doadmin(id)
{
setValue("IF_INDEX", id);
pageGetValue();
}
function setenter(e)
{
e = window.event?window.event:e;
if( e == "[object]")
{
if (event.keyCode == 13 && event.srcElement.type == "text")
{
event.keyCode = 9;
}
}
}
function onFocus()
{
getObj("Frm_Password").focus();
}
</script>
</head>
