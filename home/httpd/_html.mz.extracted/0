<%
IMPORT FILE "common_gch.gch";
var FP_IDENTITY, FP_HANDLE, FP_OBJNAME, FP_INSTNUM, GET_HANDLE, Lan_identity;
var PacketsReceived = "";
var BytesReceived   = "";
var PacketsSent     = "";
var BytesSent       = "";
var PortNum         = 0;
var Frm_MACAddress  = "";
var Frm_Status      = "";
var Frm_IPAddress = "";
var BRGRP_INSTNUM = 1;
BRGRP_INSTNUM = query_list("OBJ_BRGRP_ID", "IGD");
for (var i=0; i<BRGRP_INSTNUM; i++)
{
var BRGRP_HANDLE = create_paralist();
var BRGRP_IDENTITY = query_identity(i);
get_inst(BRGRP_HANDLE, "OBJ_BRGRP_ID", BRGRP_IDENTITY);
Frm_IPAddress = get_para(BRGRP_HANDLE, "IPAddr");
if (Frm_IPAddress==""||Frm_IPAddress==null)
{
Frm_IPAddress = "0.0.0.0";
}
destroy_paralist(BRGRP_HANDLE);
}
var lanname[10] = {0};
create_form_start("fSubmit", "");
FP_OBJNAME = "OBJ_ETH_ID";
FP_INSTNUM = 1;
FP_INSTNUM = query_list(FP_OBJNAME, "IGD.LD1");
create_hidden_sep("IF_INSTNUM", FP_INSTNUM);
for (var i=0; i<FP_INSTNUM; i++)
{
var identity = query_identity(i);
if(i == 0)
{
Lan_identity=identity;
}
var WL_HANDLE = create_paralist();
get_inst(WL_HANDLE, "OBJ_WANLAN_ID", identity);
var WLname = get_para(WL_HANDLE, "Name");
lanname[i] = WLname ;
create_special_hidden_multiinstpara(WL_HANDLE, "LAN", "Name", i);
destroy_paralist(WL_HANDLE);
}
query_list(FP_OBJNAME, "IGD.LD1");
for (var i=0; i<FP_INSTNUM; i++)
{
var identity = query_identity(i);
}
query_list(FP_OBJNAME, "IGD.LD1");
for (var i=0; i<FP_INSTNUM; i++)
{
PortNum = i+1;
var identity = query_identity(i);
var FP_HANDLE = create_paralist();
get_inst(FP_HANDLE, FP_OBJNAME, identity);
PacketsReceived = get_para(FP_HANDLE, "PacketsReceived");
create_special_hidden_multiinstpara(FP_HANDLE, "PacketsReceived", "PacketsReceived", i);
BytesReceived   = get_para(FP_HANDLE, "BytesReceived");
create_special_hidden_multiinstpara(FP_HANDLE, "BytesReceived", "BytesReceived", i);
PacketsSent     = get_para(FP_HANDLE, "PacketsSent");
create_special_hidden_multiinstpara(FP_HANDLE, "PacketsSent", "PacketsSent", i);
BytesSent       = get_para(FP_HANDLE, "BytesSent");
create_special_hidden_multiinstpara(FP_HANDLE, "BytesSent", "BytesSent", i);
Frm_MACAddress  = get_para(FP_HANDLE, "MACAddress");
create_special_hidden_multiinstpara(FP_HANDLE, "MACAddress", "MACAddress", i);
Frm_Status      = get_para(FP_HANDLE, "Status");
create_special_hidden_multiinstpara(FP_HANDLE, "Status", "Status", i);
if (Frm_MACAddress==""||Frm_MACAddress==null)
{
Frm_MACAddress = "00:00:00:00:00";
}
if (Frm_Status == "Up")
{
Frm_Status = "&?wan061;";
}
else if (Frm_Status == "NoLink")
{
Frm_Status = "&?wan062;";
}
else if (Frm_Status == "Disabled")
{
Frm_Status = "&?wan064;";
}
else
{
Frm_Status = "&?wan063;";
}
destroy_paralist(FP_HANDLE);
}
