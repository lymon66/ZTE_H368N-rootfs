if(PPP_ConnError=="ERROR_NONE")
{
PPP_ConnError="";
}
else if(PPP_ConnError=="ERROR_ISP_TIME_OUT")
{
PPP_ConnError="&?wan069;";
}
else if(PPP_ConnError=="ERROR_USER_DISCONNECT")
