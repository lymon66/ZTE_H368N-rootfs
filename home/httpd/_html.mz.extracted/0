<SCRIPT>
function pageLoad(url)
{
getObj("fSubmit").action = url;
showTable();
showBr0AddrTable();
}
function showBr0AddrTable()
{
var newline = getObj("BR0_Table").insertRow(-1);
   newline.setAttribute("class", "white");
newline.setAttribute("className", "white");
newline.insertCell(-1).innerHTML = "&?eth_007;";
cellId = "Line_0_" + "IPV4";
newline.insertCell(-1).innerHTML = getNameValue(cellId, 55, getValue("IPv4Addr"));
if(getValue("IPv6_Enable") == "1" || getValue("IPV6SETFROM") == "1")
{
newline = getObj("BR0_Table").insertRow(-1);
       newline.setAttribute("class", "white");
newline.setAttribute("className", "white");
newline.insertCell(-1).innerHTML = "&?eth_008;";
cellId = "Line_0_" + "IPV6";
if(getValue("IPV6Addr") == "::")
{
newline.insertCell(-1).innerHTML = "";
}
else
{
var ipv6Addr=getValue("IPV6Addr")+"/"+getValue("IPV6PreLen");
newline.insertCell(-1).innerHTML = getNameValue(cellId, 55, ipv6Addr);
}
newline = getObj("BR0_Table").insertRow(-1);
       newline.setAttribute("class", "white");
newline.setAttribute("className", "white");
newline.insertCell(-1).innerHTML = "&?eth_009;";
cellId = "Line_0_" + "IPV6";
if(getValue("IPV6GuaAddr") == "::")
{
newline.insertCell(-1).innerHTML = "";
}
else
{
var ipv6GuaAddr=getValue("IPV6GuaAddr")+"/"+getValue("IPV6GuaPrefLen");
newline.insertCell(-1).innerHTML = getNameValue(cellId, 55, ipv6GuaAddr);
}
}
}
function pageReFresh()
{
jslDisable("Btn_ReFresh");
getObj("fSubmit").submit();
}
function getNameValue(id, size, value)
{
return ("<input type=\"text\" class=\"uiNoBorder\" size=\"" + size +"\" value=\""
+ value + "\"  id=\"" + id + "\"  readonly>");
}
function addline(index)
{
var newline = getObj("LAN_Table").insertRow(-1);
    newline.setAttribute("class", "white");
newline.setAttribute("className", "white");
cellId = "Line_" + index + "_" + "LAN";
newline.insertCell(-1).innerHTML = getNameValue(cellId, 6, getValue("LAN"+index));
cellId = "Line_" + index + "_" + "Status";
newline.insertCell(-1).innerHTML = getNameValue(cellId, 6, getValue("Status"+index));
cellId = "Line_" + index + "_" + "Received";
newline.insertCell(-1).innerHTML = getNameValue(cellId, 25, getValue("PacketsReceived"+index) + "/" + getValue("BytesReceived"+index));
cellId = "Line_" + index + "_" + "Sent";
newline.insertCell(-1).innerHTML = getNameValue(cellId, 25, getValue("PacketsSent"+index) + "/" + getValue("BytesSent"+index));
}

function showTable()
{
var num = getValue("IF_INSTNUM");
if(num != 0 )
{
for(var i = 0; i < num; i++)
{
addline(i);
}
}
else
{
var newline = getObj("LAN_Table").insertRow(-1);
newline.setAttribute("align", "center");
newline.setAttribute("class", "white");
newline.setAttribute("className", "white");
var objcell =  newline.insertCell(-1);
for(var i = 0; i < 3; i++)   
        {
var objcelltmp = newline.insertCell(-1);
objcelltmp.parentNode.removeChild(objcelltmp);
}
objcell.colSpan = i + 1;
objcell.innerHTML= "&?note_008;"
}
}
