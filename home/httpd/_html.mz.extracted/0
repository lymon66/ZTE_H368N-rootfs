{
CurUpLinkType="ADSL";
}
else if (CurUpLinkType==2)
{
CurUpLinkType="VDSL";
}
else if (CurUpLinkType==3)
{
CurUpLinkType="Ethernet";
}
else if (CurUpLinkType==4)
{
