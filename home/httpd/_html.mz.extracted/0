<script language="javascript">var gDMZ4Num=0;var gDMZ6Num=0;var ipv6_index =-1;var USERIF_PARA=new Array("ViewName","Enable","InternalHost","DeviceName");<%if(getenv("IPv6_Enable")=="1"||getenv("IPV6SETFROM")=="1"){%>var FWDMZ6_PARA=new Array("Enablev6","InternalV6Host","InternalMacHost","MacEnable","DeviceName");<%}%>function getmsg(id){var str=new Array();str[0]=new Array(101,"&?error_101;");str[1]=new Array(108,"&?error_108;");str[2]=new Array(118,"&?error_118;");str[3]=new Array(119,"&?error_119;");str[4]=new Array(122,"&?error_122;");str[5]=new Array(141,"&?error_141;");str[6]=new Array(155,"&?error_155;");str[7]=new Array(156,"&?error_156;");str[8]=new Array(915,"&?error_915;");str[9]=new Array(918,"&?error_918;");str[10]=new Array(919,"&?error_919;");str[11]=new Array(110,"&?error_110;");return getMsgFormArray(str,arguments);}function pageLoad(url){getObj("fSubmit").action=url;moveErrLayer(580,95);var errstr=getValue("IF_ERRORSTR");var errpara=getValue("IF_ERRORPARAM");OldProcessRet(errstr,errpara);IPMode_Switch();pageGetValue();SetEnableState();}function SetEnableState(){if(gDMZ4Num>0){jslEnable("Frm_Enable");}else {jslDisable("Frm_Enable");}<%if(getenv("IPv6_Enable")=="1"||getenv("IPV6SETFROM")=="1"){%>if(gDMZ6Num>0){jslEnable("Frm_Enablev6");}else {jslDisable("Frm_Enablev6");}<%}%>}function IPMode_Switch(){setValue("Frm_InternalHost","");var objInputMode=getObj("Frm_InputMode");for(var index =0;index <objInputMode.options.length;index++){objInputMode.remove(index);index--;}<%if(getenv("IPv6_Enable")=="1"||getenv("IPV6SETFROM")=="1"){%>var IPMode=getValue("Frm_IPMode");<%}else {%>var IPMode="IPv4";<%}%>if("IPv6"==IPMode){addOptionByValueText("Frm_InputMode","IP","IP Address");addOptionByValueText("Frm_InputMode","Device_IP","Access Device");addOptionByValueText("Frm_InputMode","MAC","MAC Address");getObj("Fnt_InternalHost").innerHTML ="&?dmco007;";}else {addOptionByValueText("Frm_InputMode","IP","IP Address");addOptionByValueText("Frm_InputMode","Device_IP"